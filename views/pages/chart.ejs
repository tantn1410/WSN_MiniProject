<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.7.0/chart.min.js" integrity="sha512-TW5s0IT/IppJtu76UbysrBH9Hy/5X41OTAbQuffZFU6lQ1rdcLHzpU5BzVvr/YFykoiMYZVWlr/PX1mDcfM9Qg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script>
const humid = document.getElementById('humid');
const humidChart = new Chart(humid, {
    type: 'line',
    data: {
        labels: Array(60).fill(''),
        datasets: [
            {
                label: 'Humid',
                data: [],
                borderColor: [
                'rgba(17, 113, 213, 0.8)',
                ],
                // fill: true,
                backgroundColor: [
                'rgba(17, 113, 213, 0.8)',
                ],
                borderWidth: 1
            }
        ]
    },
    options: {
        responsive: true,
        maintainAspectRatio: false,
        scales: {
            y: {
                title: {
                    display: true,
                    text: '%',
                    align: 'end'
                },
    },
                x: {
                    title: {
                        display: true,
                        text: 'Time (s)',
                        align: 'end'
                    }
                }
        }
    }
});
const temp = document.getElementById('temp');
const tempChart = new Chart(temp, {
    type: 'line',
    data: {
        labels: Array(60).fill(''),
        datasets: [
            {
                label: 'Temperature',
                data: [],
                borderColor: [
                    'rgba(255, 99, 71, 1)',
                ],
                // fill: true,
                backgroundColor: [
                    'rgba(255, 99, 71, 1)',
                ],
                borderWidth: 1
            }
        ]
    },
    options: {
        responsive: true,
        maintainAspectRatio: false,
        scales: {
            y: {
                title: {
                    display: true,
                    text: 'Celcius',
                    align: 'end'
                },
    },
                x: {
                    title: {
                        display: true,
                        text: 'Time (s)',
                        align: 'end'
                    }
                }
        }
    }
});
</script>